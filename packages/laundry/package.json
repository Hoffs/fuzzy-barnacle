  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-relay": "^11.0.1",
    "@types/relay-runtime": "^11.0.0",
    "babel-plugin-relay": "^11.0.2",
    "eslint-config-prettier": "^8.1.0",
    "get-graphql-schema": "^2.1.2",
    "prettier": "2.2.1",
